let primeNumbers = [];
function solution(n) {
    var answer = 1;
    for (let i = 2; i <= n; i++) {
        if (i % 2 === 0) continue;
        if (isPrimeNumber(i)) answer++;
    }
    return answer;
}

function isPrimeNumber(n) {
    for (let i = 2; i <= Math.sqrt(n, 2) ; i++) {
        if (i % 2 === 0) continue;
        if (n % i === 0) return false;
    }
    return true;
}
